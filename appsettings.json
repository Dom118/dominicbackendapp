{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "GitHub": {
    "ClientId": "${GitHub_ClientId}",
    "ClientSecret": "${GitHub_ClientSecret}"
  },
  "Jwt": {
    "Secret": "${Jwt_Secret}",
    "Issuer": "myIssuer",
    "Audience": "myAudience"
  }
   },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DefaultConnection}",
    "AzureBlobStorage": "${AzureBlobStorage}"
  }
  
}
